{
  "permissions": ["activeTab"],
  "options_ui": {
    "page": "src/options.html"
  },
  "description": "Retain essential content, remove unwanted noise.",
  "manifest_version": 2,
  "name": "recrun",
  "version": "3.3.0",
  "browser_action": {
    "default_icon": "icons/icon38.png"
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": [
      "src/background.js"
    ]
  },
  "web_accessible_resources": [
    "src/loader.gif",
    "src/style.css",
    "src/options.html",
    "src/iframe.html",
    "src/content.html",
    "src/lib/jquery.js"
  ]
}
